{
    "name": "shaulhaf/parent-image",
    "description": "Laravel Nova tools for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^5.6",
        "spatie/laravel-medialibrary": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Capitalc\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Capitalc\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
